def country = "nigera"

pipeline {
	agent any
        triggers { 
            cron('H/1 * * *')
        }
        stages {
   
	    stage('build') {
                             
			steps {
				//sh 'ansible-playbook yaml.yaml -i inventory'
                                 sh "echo ${country}"
                                 sh 'touch lorence'
			}
		}
	}
}

