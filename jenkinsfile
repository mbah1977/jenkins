def country = "nigera"

pipeline {
	agent any
        parameters {
           string(woman: 'man')
        }
        stages { 
	    stage('build') {
                   input {
                      message " Should we"
                      ok" yes"
                   }
                   steps {
	             sh "echo ${country}" 
                     sh "echo ${woman}"
                }                 
            }
    }        
}

